          }
        },
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "color": "green", "value": null }
          ]
        }
      },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "targets": [
